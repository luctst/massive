{
  "kind": "collectionType",
  "collectionName": "medias",
  "info": {
    "singularName": "media",
    "pluralName": "medias",
    "displayName": "Media",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 50,
      "required": true
    },
    "views": {
      "type": "biginteger",
      "default": "0"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "description": {
      "type": "richtext"
    },
    "preview": {
      "type": "string",
      "regex": "        RegExp pattern: [(http(s)?):\\/\\/(www\\.)?a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "media"
    }
  }
}
